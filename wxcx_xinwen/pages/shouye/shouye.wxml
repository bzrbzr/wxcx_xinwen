<view class = "shouye">
  <swiper indicator-dots="true" autoplay="true" >
  <view wx:for="{{swiperimage}}" wx:key="swiper{{index}}">
      <swiper-item>
      <image src="{{item.src}}"></image>
    </swiper-item>
  </view>
  </swiper>
  <view class="xinwenlist">
    <view class="new-item" wx:for='{{newslist}}' wx:key='{{item.id}}'>
      <image src='{{item.poster}}'></image>
      <text>{{item.title}}----{{item.data}}</text>
    </view>
  </view>
</view>